                        </tr>
                    </thead>
                    <tbody>
                        <% for (let i = 0, size = top_revisions.length; i < size; i++) {%>
                            <tr>
                                <td><%= bottom_revisions[i]._id%></td>
                                <td><%= bottom_revisions[i].numOfEdits%></td>
                            </tr>
                        <% } %>                        
                    </tbody>
                    
                </table>               
            </div>

